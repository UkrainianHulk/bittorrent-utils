    "scripts": {
        "dev": "cross-env NODE_ENV=dev nodemon index.js",
        "dev-manual-transfer": "cross-env NODE_ENV=dev nodemon ./src/manualTransfer.js",
        "start": "npm install --omit=dev && node -e \"console.clear()\" && cross-env NODE_ENV=user node index.js",
        "start-manual-transfer": "npm install --omit=dev && node -e \"console.clear()\" && cross-env NODE_ENV=user node ./src/manualTransfer.js",
        "lint": "prettier --check . && eslint **/*.js",
        "format": "prettier --write . && eslint --fix **/*.js"
    },
    "author": "Yaroslav Sorochan <yaroslav.sorochan.i@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.2.3",
        "eslint-plugin-promise": "^6.0.0",
        "nodemon": "^2.0.12",
        "prettier": "2.7.1"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.3.7",
        "@grpc/proto-loader": "^0.6.4",
        "b58": "^4.0.3",
        "colors": "^1.4.0",
        "config": "^3.3.6",
        "cross-env": "^7.0.3",
        "keccak": "^3.0.2",
